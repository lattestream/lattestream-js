{
  "name": "lattestream-js",
  "private": true,
  "description": "LatteStream JavaScript/TypeScript SDK monorepo",
  "keywords": [
    "websocket",
    "realtime",
    "sdk",
    "lattestream"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lattestream/lattestream-js.git"
